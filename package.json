{
  "name": "tree-sitter-solidity",
  "version": "1.0.5",
  "description": "A tree sitter parser for Solidity",
  "main": "bindings/node",
  "scripts": {
    "test": "tree-sitter generate && tree-sitter test"
  },
  "author": "Joran Honig",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.14.1"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.16.9"
  },
  "tree-sitter": [
    {
      "scope": "source.sol",
      "file-types": [
        "sol"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "injection-regex": "^(sol|solidity)$"
    }
  ]
}
