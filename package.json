{
  "name": "tree-sitter-solidity",
  "version": "1.0.13",
  "description": "A tree sitter parser for Solidity",
  "main": "bindings/node",
  "types": "index.d.ts",
  "scripts": {
    "test": "tree-sitter generate && tree-sitter test"
  },
  "author": "Joran Honig",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.15.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.6"
  },
  "keywords": [
    "solidity",
    "ethereum",
    "tree sitter",
    "parser",
    "evm"
  ],
  "bugs": {
    "url": "https://github.com/JoranHonig/tree-sitter-solidity/issues"
  },
  "homepage": "https://github.com/JoranHonig/tree-sitter-solidity#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoranHonig/tree-sitter-solidity.git"
  },
  "tree-sitter": [
    {
      "scope": "source.sol",
      "file-types": [
        "sol"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "injection-regex": "^(sol|solidity)$"
    }
  ]
}
