{
  "grammars": [
    {
      "name": "solidity",
      "camelcase": "Solidity",
      "scope": "source.sol",
      "path": ".",
      "file-types": [
        "sol"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "injection-regex": "^(sol|solidity)$"
    }
  ],
  "metadata": {
    "version": "1.2.8",
    "license": "MIT",
    "description": "A tree sitter parser for Solidity",
    "authors": [
      {
        "name": "Joran Honig"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-solidity"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
